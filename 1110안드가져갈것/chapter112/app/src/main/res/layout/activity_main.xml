        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="예제 11-4"/>

    <Button
        android:id="@+id/thirdBtn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="실습 11-1"/>

    <Button
        android:id="@+id/fourthBtn"
        android:layout_width="wrap_content"
